@model SyndicateIT.Model.ViewModel.ServiceManagement.ServicesContentViewModel

@Html.Partial("~/Views/Shared/_NavigationPartial.cshtml", @Model.Navigation)
<script src="~/Scripts/Views/Services/services.js"></script>
<div id="content">
    @Html.Hidden("UserType",Model.ServiceType)
    <div class="row">
        <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
            <h1 class="page-title txt-color-blueDark">
                <i class="@Model.ClassTitle"></i> @Model.Title<span></span>
            </h1>
        </div>
       
        @if (Model.ServiceType == ServiceType.Agenda)
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <!-- Button trigger modal -->
                <a data-toggle="mo
                   dal" href="#myModal" onclick="LoadAgendaDetails();" class="btn btn-success  btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> View Daily @ViewBag.UserType</a>
            </div>
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.Attendance)
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.AbsorptionGrades)
        {
           
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.DegreesParticipationActivites)
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.Duties)
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <!-- Button trigger modal -->
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary  btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.LevelBehavior)
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <!-- Button trigger modal -->
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary  btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
        else if (Model.ServiceType == ServiceType.LevelIntelligence)
        {
           
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                <a data-toggle="modal" href="#myModal" onclick="LoadServicesContentDetails();" class="btn btn-primary btn-lg pull-right header-btn hidden-mobile"><i class="fa fa-circle-arrow-up fa-lg"></i> Add New @ViewBag.UserType</a>
            </div>
        }
             
        else
        {
            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">&nbsp; </div>
        }
       
    </div>
    <section id="Gender-content" class="">
            <div class="row">
                <!-- NEW WIDGET START -->
                <article class="col-sm-12 sortable-grid ui-sortable">
                    <!-- Widget ID (each widget will need unique ID)-->
                    <div class="jarviswidget jarviswidgetTransparent" id="wid-id-x" data-widget-colorbutton="false" data-widget-editbutton="false" data-widget-togglebutton="false" data-widget-deletebutton="false" data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false" role="widget">
                        <header role="heading">
                            <span class="widget-icon"> <i class="fa fa-align-justify"></i> </span>
                            <h2>Search</h2>
                            <span class="jarviswidget-loader"><i class="fa fa-lg fa-fw fa-th-list"></i></span>
                        </header>
                        <!-- widget div-->
                        <div role="content">
                            <div class="row">
                                @Html.Partial("~/Views/ServiceManagement/_ServicesSearchPartial.cshtml", @Model.ServicesSearch)
                            </div>
                        </div>
                        <!-- end widget content -->
                    </div>
                    <!-- end widget -->
                </article>
                <!-- WIDGET END -->
                <!-- NEW WIDGET START -->
                <!-- WIDGET END -->
            </div>
        </section>
    <section id="widget-grid" class="">
            <div class="row">
                <!-- NEW WIDGET START -->
                <article class="col-xs-12 col-sm-12 col-md-12 col-lg-12 sortable-grid ui-sortable">
                    <!-- Widget ID (each widget will need unique ID)-->
                    <div class="jarviswidget jarviswidget-color-blueDark jarviswidget-sortable" id="wid-id-0" data-widget-editbutton="false" role="widget">
                        <header role="heading">
                            <div class="jarviswidget-ctrls" role="menu">
                                <a href="javascript:void(0);" class="button-icon jarviswidget-toggle-btn" rel="tooltip" title="" data-placement="bottom" data-original-title="Collapse">
                                    <i class="fa fa-minus "></i>
                                </a>
                                <a href="javascript:void(0);" class="button-icon jarviswidget-fullscreen-btn" rel="tooltip" title="" data-placement="bottom" data-original-title="Fullscreen">
                                    <i class="fa fa-expand "></i>
                                </a>
                            </div>
                            <span class="widget-icon"> <i class="@Model.ClassTitle"></i> </span>
                            <h3 class="widget-h3">  List</h3>
                            <span class="jarviswidget-loader"><i class="fa fa-refresh fa-spin"></i></span>
                        </header>
                        <!-- widget div-->
                        <div role="content" id="Grid_User">
                            <!-- widget edit box -->
                            <div class="jarviswidget-editbox">
                                <!-- This area used as dropdown edit box -->
                            </div>
                            <!-- end widget edit box -->
                            <!-- widget content -->
                            <div class="widget-body " id="Service-List">
                            </div>
                            <!-- end widget content -->
                        </div>
                        <!-- end widget div -->
                    </div>
                    <!-- end widget -->
                </article>
            </div>
        </section>
</div>
<script type="text/javascript">
    $(document).ready(function () {
        if ('@Model.ServiceType' == '@ServiceType.Duties') {
            $("#StudentLevelsUL").children().find('li').removeClass("active");
            $("#Duties").addClass("active");
            $("#StudentLevels").addClass("open"),
            $("#StudentLevelsUL").show();
            $("#StudentLevels").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentLevels").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.LevelBehavior') {
            $("#StudentLevelsUL").children().find('li').removeClass("active");
            $("#LevelBehavior").addClass("active");
            $("#StudentLevels").addClass("open"),
            $("#StudentLevelsUL").show();
            $("#StudentLevels").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentLevels").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.LevelIntelligence') {
            $("#StudentLevelsUL").children().find('li').removeClass("active");
            $("#LevelIntelligence").addClass("active");
            $("#StudentLevels").addClass("open"),
            $("#StudentLevelsUL").show();
            $("#StudentLevels").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentLevels").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.Attendance') {
            $("#StudentGradesUL").children().find('li').removeClass("active");
            $("#Attendance").addClass("active");
            $("#StudentGrades").addClass("open"),
            $("#StudentGradesUL").show();
            $("#StudentGrades").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentGrades").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.DegreesParticipationActivites') {
            $("#StudentGradesUL").children().find('li').removeClass("active");
            $("#DegreesParticipation").addClass("active");
            $("#StudentGrades").addClass("open"),
            $("#StudentGradesUL").show();
            $("#StudentGrades").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentGrades").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.AbsorptionGrades') {
            $("#StudentGradesUL").children().find('li').removeClass("active");
            $("#AbsorptionGrades").addClass("active");
            $("#StudentGrades").addClass("open"),
            $("#StudentGradesUL").show();
            $("#StudentGrades").children().find('em').removeClass("fa fa-plus-square-o");
            $("#StudentGrades").children().find('em').addClass("fa fa-minus-square-o");
        }

        else if ('@Model.ServiceType' == '@ServiceType.Agenda') {
            $("#ServicesUL").children().find('li').removeClass("active");
            $("#Agenda").addClass("active");
            $("#Services").addClass("open"),
            $("#ServicesUL").show();
            $("#Services").children().find('em').removeClass("fa fa-plus-square-o");
            $("#Services").children().find('em').addClass("fa fa-minus-square-o");
        }
        else if ('@Model.ServiceType' == '@ServiceType.Schedule') {
            $("#ServicesUL").children().find('li').removeClass("active");
            $("#Schedule").addClass("active");
            $("#Services").addClass("open"),
            $("#ServicesUL").show();
            $("#Services").children().find('em').removeClass("fa fa-plus-square-o");
            $("#Services").children().find('em').addClass("fa fa-minus-square-o");
        }
        else if ('@Model.ServiceType' == '@ServiceType.PeriodSchedule') {
            $("#ServicesUL").children().find('li').removeClass("active");
            $("#PeriodSchedule").addClass("active");
            $("#Services").addClass("open"),
            $("#ServicesUL").show();
            $("#Services").children().find('em').removeClass("fa fa-plus-square-o");
            $("#Services").children().find('em').addClass("fa fa-minus-square-o");
        }
        else if ('@Model.ServiceType' == '@ServiceType.DistributionStudents') {
            $("#ServicesUL").children().find('li').removeClass("active");
            $("#DistributionStudents").addClass("active");
            $("#Services").addClass("open"),
            $("#ServicesUL").show();
            $("#Services").children().find('em').removeClass("fa fa-plus-square-o");
            $("#Services").children().find('em').addClass("fa fa-minus-square-o");
        } else if ('@Model.ServiceType' == '@ServiceType.ExamProgram') {
            $("#ServicesUL").children().find('li').removeClass("active");
            $("#ExamProgram").addClass("active");
            $("#Services").addClass("open"),
            $("#ServicesUL").show();
            $("#Services").children().find('em').removeClass("fa fa-plus-square-o");
            $("#Services").children().find('em').addClass("fa fa-minus-square-o");
        }
    });
</script>
@section Scripts {
    @Scripts.Render("~/bundles/DropDopDistributionStudents")
    @Scripts.Render("~/bundles/jqueryval")
}
