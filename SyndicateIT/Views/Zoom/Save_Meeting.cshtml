@model SyndicateIT.DataLayer.DataContext.T_Meeting
@{
    var users = ViewData["users"] as List<SelectListItem>;

}
<script src="https://cdn.jsdelivr.net/npm/signature_pad@4.0.0/dist/signature_pad.umd.min.js"></script>
@if (ViewData["error"] != null)
{
    <div>@ViewData["error"]</div>
}
<form method="post" class="col-md-8" action="~/Zoom/SAve_Meeting">
    <div class="form-group">
        @Html.Label("topic")
        @Html.TextBox("topic", "", new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.Label("Users", "Users")
        @Html.DropDownList("users", users, new { @class = "form-control", multiple = "multiple" })
    </div>
    <button type="submit">vote</button>
</form>
