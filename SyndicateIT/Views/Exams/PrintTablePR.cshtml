@model SyndicateIT.Model.ViewModel.Exam.TablePRViewModel
@{
    Layout = null;
    string PublicResources = "~/Views/Exams/SignsExam/_TablePRExamPartial.cshtml";
}
<table width="100%" cellpadding="0" cellspacing="0" style="border-collapse: collapse;border-spacing: 0;border-color: grey;">
    <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
        <td style="vertical-align: top;padding: 0; display: table-cell; vertical-align: inherit;">
            <table width="100%" cellpadding="0" cellspacing="0" style="background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                    <td style="line-height: 1.42857143; vertical-align: top;width: 39.33333333%; min-height: 1px;padding-left: 13px; padding-right: 13px;padding: 4px 5px; display: table-cell; vertical-align: inherit;">
                        <table width="100%" cellpadding="0" cellspacing="0" style="background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;display: table-cell; vertical-align: inherit;padding: 4px 5px;text-align: right;font-size:16px;font:bold;">@Model.Class</td>
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;display: table-cell; vertical-align: inherit;padding: 4px 5px;font-size:16px; font:bold;">@Html.Resource("Class", PublicResources)</td>
                            </tr>
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;text-align: right;">@Model.Division</td>
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;padding: 4px 5px; font-size:16px;font:bold;">@Html.Resource("Division", PublicResources)</td>
                            </tr>
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;padding: 4px 5px;text-align: right;">@Model.NumberStudent</td>
                                <td style="display: table-cell; vertical-align: inherit;line-height: 1.42857143; vertical-align: top;padding: 4px 5px;font-size:16px;font:bold;">@Html.Resource("NumberStudents", PublicResources)</td>
                            </tr>
                        </table>
                    </td>
                    <td style="width: 27.33333333%; min-height: 1px;padding-left: 13px; padding-right: 13px;padding: 4px 5px; display: table-cell; vertical-align: inherit;"><img src="http://www.byec-edu.org/AppByec/Content/image/Corporate/logo-bey-exam1.png" style="width:90px!important; height:90px!important;"></td>
                    <td style="width: 33.33333333%; min-height: 1px;padding-left: 13px; padding-right: 13px;padding: 4px 5px; display: table-cell; vertical-align: inherit;">
                        <table width="100%" cellpadding="0" cellspacing="0" style="background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                <td colspan="2" class="txt-center font-xl" style="vertical-align: top;display: table-cell; vertical-align: inherit;text-align:center;font-size:18px;font:bold;">@Html.Resource("SchoolNameAr", PublicResources)</td>
                            </tr>
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                <td colspan="2" class="txt-center font-xl" style="vertical-align: top;display: table-cell; vertical-align: inherit;text-align:center;font-size:18px;font:bold;">@Html.Resource("SchoolName", PublicResources)</td>
                            </tr>
                            <tr style="display: table-row; vertical-align: inherit; border-color: inherit;vertical-align: bottom; ">
                                <td style="line-height: 1.42857143; vertical-align: bottom;display: table-cell; vertical-align: inherit;text-align: right;font-size:18px;font:bold;">@Model.NameStudent</td>
                                <td style="line-height: 1.42857143; vertical-align: bottom;display: table-cell; vertical-align: inherit;text-align: right;font-size:18px;font:bold;" class="font-md">@Html.Resource("Name", PublicResources)</td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
        <td class="col-md-12" style="width: 100%; float: left;position: relative; min-height: 1px;padding-left: 13px;padding-right: 13px;display: table-cell; vertical-align: inherit;padding: 4px 5px;">
            <table cellpadding="0" cellspacing="0" style="width: 100%; float: left;position: relative; min-height: 1px;padding-left: 13px;padding-right: 13px;background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; display: table-cell; vertical-align: inherit;text-align:center;font-size:20px;">
                        @Html.Resource("SignExamTitle", PublicResources)  @Model.ClassNameAlfa
                    </td>
                </tr>
            </table>
        </td>
    </tr>
    <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
        <td style="padding: 4px 5px; display: table-cell; vertical-align: inherit;">
            <table width="100%" cellpadding="0" cellspacing="0">
                <tr>
                    <td>
                        <table width="100%" cellpadding="0" cellspacing="0">
                            @for (int i = 0; i < Model.ItemSubjectPR.Count; i++)
                            {
                                var dir = "";
                                var txtAlign = "";
                                if (i == 0)
                                {
                                    dir = "rtl";
                                    txtAlign = "text-align:right";
                                }
                                else
                                {
                                    dir = "ltr";
                                    txtAlign = "text-align:left";
                                }

                                <tr dir=@dir>
                                    <td dir=@dir><h3>@Model.ItemSubjectPR[i].Subject</h3></td>
                                </tr>
                                for (int t = 0; t < Model.ItemSubjectPR[i].ItemCoursePR.Count; t++)
                                {
                                    var ItemCourseData = Model.ItemSubjectPR[i].ItemCoursePR[t];
                                    <tr>
                                        <td>
                                            <table style="width: 100%; float: left;background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;" dir=@dir>
                                                <thead>
                                                    <tr>
                                                        <th style="width:53%;text-align:center;background-color: rgba(61, 106, 124, 0.15);border: 1px solid rgba(61, 106, 124, 0.2); font-size:16px;">@ItemCourseData.Name</th>
                                                        <th style="width:17%;text-align:center;background-color: rgba(61, 106, 124, 0.15);border: 1px solid rgba(61, 106, 124, 0.2);font-size:16px;">@ItemCourseData.FirstTerm</th>
                                                        <th style="width:17%;text-align:center;background-color: rgba(61, 106, 124, 0.15);border: 1px solid rgba(61, 106, 124, 0.2);font-size:16px;">@ItemCourseData.SecondTerm </th>
                                                        <th style="width:17%;text-align:center;background-color: rgba(61, 106, 124, 0.15);border: 1px solid rgba(61, 106, 124, 0.2);font-size:16px;">@ItemCourseData.ThirdTerm </th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @for (int j = 0; j < ItemCourseData.ItemSkilsPR.Count; j++)
                                                    {

                                                        <tr style="background-color: #fff;">
                                                            <th style=@txtAlign>@ItemCourseData.ItemSkilsPR[j].Name</th>
                                                            <td style=@txtAlign>@ItemCourseData.ItemSkilsPR[j].FirstTermValue</td>
                                                            <td style=@txtAlign>@ItemCourseData.ItemSkilsPR[j].SecondTermValue</td>
                                                            <td style=@txtAlign>@ItemCourseData.ItemSkilsPR[j].ThirdTermValue</td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </td>
                                    </tr>
                                }

                            }

                        </table>
                    </td>
                </tr>

            </table>
        </td>
    </tr>
    <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
        <td class="col-md-12" style="width: 100%; float: left;position: relative; min-height: 1px;margin-bottom:30px;margin-left:50px; text-align:left;margin-top:0px;font-size:16px;">
            @Html.Resource("AdminSignature", PublicResources)
        </td>
    </tr>
    <tr>
        <td><hr style="border-top: 2px dashed #000; width:95%; float:left;" /><img src="http://www.byec-edu.org/AppByec/Content/image/Repair.png" style="width:20px!important; height:20px!important;"></td>
    </tr>
    <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
        <td style="width: 100%; float: left;">
            <table style="width: 100%; float: left;background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                <tr>
                    <td style="width: 70%;border: 1px solid #ddd;">
                        <table width="100%" cellpadding="0" cellspacing="0" style="background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                            <tbody style="display: table-row-group; vertical-align: middle;border-color: inherit;">
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right; font-size:16px;">@Html.Resource("ParentsNotes", PublicResources) </td>
                                </tr>
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: left; font-size:16px;">@Html.Resource("ParentSignature", PublicResources)</td>
                                </tr>
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td>
                                        <table width="100%" cellpadding="0" cellspacing="0">
                                            <tr>
                                                <td width="68%">&nbsp;</td>
                                                <td style="width:30%;border: 1px solid #ddd;line-height: 1.42857143; vertical-align: top;margin: 4px 5px; text-align: center; font-size:16px;">@Html.Resource("ReturnNote", PublicResources)</td>
                                                <td width="2%">&nbsp;</td>
                                            </tr>
                                        </table>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td style="width: 30%;border: 1px solid #ddd;">
                        <table width="100%" cellpadding="0" cellspacing="0" style="background-color: transparent; border-collapse: collapse; border-spacing: 0; display: table; border-spacing: 0;">
                            <tbody style="display: table-row-group; vertical-align: middle;border-color: inherit;">
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right; font-size:16px;">@Model.NameStudent</td>
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right; font-size:16px;">@Html.Resource("Name", PublicResources)</td>
                                </tr>
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right;font-size:16px;">@Model.Class</td>
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right; font-size:16px;">@Html.Resource("Class", PublicResources)</td>
                                </tr>
                                <tr style="display: table-row; vertical-align: inherit; border-color: inherit;">
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right;font-size:16px;">@Model.Division</td>
                                    <td style="line-height: 1.42857143; vertical-align: top;padding: 4px 5px; text-align: right; font-size:16px;">@Html.Resource("Division", PublicResources)</td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
            </table>
        </td>
    </tr>
</table>








    


