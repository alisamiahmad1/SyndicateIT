
@model List<SyndicateIT.Model.ViewModel.FileManagement.FileAgendaDailyListViewModel>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
<div class="row">
    <div class="col-sm-12">
        <div class="well" style="background-color:#fff!important;border-color:#fff!important">
            <table class="table table-striped table-forum">
                <tbody>
                   @for (int i = 0; i < Model.Count; i++)
                   {
                      <tr>
                        <td class="text-center">
                            <a href="#" class="btn btn-default txt-color-pink"><i class="fa fa-gear fa-lg"></i>&nbsp; <strong>@Model[i].CourseName</strong></a>
                        </td>
                        <td>on <em>@Model[i].PeriodDate</em></td>
                    </tr>
                      <tr>
                        <td colspan="2">
                          
                            <h5>@Model[i].Title</h5>                           
                            <p style="margin-top:30px;">
                               @Model[i].Description.ToHTMLParagraph()
                            </p><em>
                                - @Model[i].TeacherName
                                <br>
                                 @Model[i].PostionName
                            </em>                          
                        </td>
                    </tr>
                    }              
                   <tr>
                        <td class="text-center" style="width: 12%;"></td>
                        <td>&nbsp;</td>
                    </tr>
                    <!-- end Post -->
                </tbody>
            </table>
        </div>
    </div>
</div>